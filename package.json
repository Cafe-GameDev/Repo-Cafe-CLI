  "author": "Cafe-GameDev",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "dependencies": {
    "adm-zip": "^0.5.10"
  },
  "peerDependencies": {
    "@google/gemini-cli": "latest"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
